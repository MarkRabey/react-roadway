{
  "name": "react-roadway",
  "version": "0.1.0",
  "description": "Minimal Routing for React",
  "main": "dist/index.js",
  "scripts": {
    "build:dist": "babel src -d dist",
    "build:docs": "webpack --devtool inline-source-map",
    "deploy:docs": "git add docs/dist && git commit -m \"rebuild docs\" && git subtree push --prefix docs/dist origin gh-pages",
    "build": "npm run build:dist && npm run build:docs",
    "start": "webpack-dev-server --devtool inline-source-map --progress --colors --inline --hot --content-base docs",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Opentrace/react-roadway.git"
  },
  "keywords": [
    "react",
    "router",
    "routing",
    "history"
  ],
  "author": "Mark Rabey <mark@opentrace.ca>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Opentrace/react-roadway/issues"
  },
  "homepage": "https://github.com/Opentrace/react-roadway#readme",
  "dependencies": {
    "history": "^4.6.3",
    "path-to-regexp": "^1.7.0",
    "prop-types": "^15.5.10",
    "re-flex": "^0.1.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-live": "^1.7.0"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.0",
    "webpack-dev-server": "^2.6.1"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
